{
  "name": "vanilla-extract",
  "version": "0.0.0",
  "private": true,
  "author": "SEEK",
  "license": "MIT",
  "scripts": {
    "%packages": "pnpm --filter='@vanilla-extract/*' --aggregate-output",
    "clean": "pnpm %packages exec crackle clean",
    "dev": "pnpm %packages dev",
    "build": "pnpm %packages build",
    "watch": "preconstruct watch",
    "start-fixture": "ts-node ./test-helpers/src/startFixtureCLI",
    "start": "pnpm start-fixture themed",
    "start-site": "manypkg run site start",
    "build-site": "manypkg run site build",
    "test:jest": "jest",
    "test:playwright": "playwright test",
    "format": "prettier --write .",
    "lint": "manypkg check && prettier --check . && tsc",
    "copy-readme-to-packages": "ts-node scripts/copy-readme-to-packages",
    "version": "changeset version && pnpm install --lockfile-only",
    "prepare-release": "pnpm copy-readme-to-packages && pnpm build",
    "release": "pnpm prepare-release && changeset publish"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "volta": {
    "node": "16.17.0"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.17.0",
    "@crackle/cli": "^0.12.6",
    "@manypkg/cli": "^0.19.1",
    "@playwright/test": "^1.25.1",
    "@preconstruct/cli": "^2.0.1",
    "@swc/core": "^1.2.112",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.59",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vanilla-extract/babel-plugin": "*",
    "babel-jest": "^27.3.1",
    "fast-glob": "^3.2.7",
    "jest": "^27.3.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.7.4"
  },
  "packageManager": "pnpm@7.9.5",
  "pnpm": {
    "patchedDependencies": {
      "html-render-webpack-plugin@3.0.1": "patches/html-render-webpack-plugin@3.0.1.patch"
    }
  }
}
